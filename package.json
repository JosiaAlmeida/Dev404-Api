{
  "name": "Devs-404-Api-Ts",
  "version": "1.0.0",
  "main": "src/server.ts",
  "author": "Josia Almeida <josiaalmeida0001@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "node --loader ts-node/esm 'src/server.ts'",
    "go": "ts-node-dev src/server.ts",
    "goT": "node --trace-deprecation --abort-on-uncaught-exception ts-node/register src/server.ts",
    "gogoT": "node './src/server.ts'",
    "gogo": "node --trace-warnings --experimental-specifier-resolution=node dist/server.js",
    "gogoI": "nodemon --trace-warnings --experimental-specifier-resolution=node dist/server.js",
    "gobabel": "node dist/server.js",
    "migration": "typeorm migration:run",
    "goTsNode": "nodemon --watch 'src/' --exec \"ts-node src/server.ts\" -e ts"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.16.5",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mysql": "^2.18.1",
    "pdfmake": "^0.2.4",
    "sqlite3": "^5.0.2",
    "ts-node": "^10.4.0",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/module-alias": "^2.0.1",
    "@types/pdfmake": "^0.1.19",
    "@types/uuid": "^8.3.3",
    "babel-core": "^6.26.3",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
